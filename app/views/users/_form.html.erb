<%=form_for user do |f|%>
  <%= f.label :name%>
  <%= f.text_field :name%><br>

  <%= f.label :email%>
  <%= f.text_field :email%><br>

  <%= f.label :password%>
  <%= f.password_field :password%><br>

  <%= f.label :password_confirm%>
  <%= f.password_field :password_confirm%><br>
  
  <%= f.submit "Create Account"%>
<%end%>
